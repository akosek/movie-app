{
  "expo": {
    "name": "Assignment React Native",
    "slug": "assignment-react-native",
    "version": "0.0.1",
    "privacy": "hidden",
    "scheme": "app",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "splash": {
      "image": "./assets/appSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#505137"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.noaignite.assignment",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.noaignite.assignment",
      "versionCode": 1,
      "permissions": []
    }
  }
}
