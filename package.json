{
    "main": "node_modules/expo/AppEntry.js",
    "author": "Noa Ignite Denmark",
    "name": "assignment-react-native",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "build:android": "npm run bump-build-number && expo build:android -t app-bundle --release-channel test",
        "build:android:prod": "npm run bump-build-number && PROD=true npm run build:android",
        "build:ios": "npm run bump-build-number && expo build:ios --release-channel test",
        "build:ios:prod": "npm run bump-build-number && PROD=true npm run build:ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "prettier": "prettier --write .",
        "type-gen": "npx swagger-typescript-api -p [SWAGGER_ENDPONT] --route-types true --output ./src/lib --name apiTypeDefs.ts --no-client && prettier --write src/lib/apiTypeDefs.ts",
        "bump-build-number": "node ./scripts/bump-build-number.js"
    },
    "dependencies": {
        "@nandorojo/swr-react-native": "^0.1.3",
        "@react-native-async-storage/async-storage": "~1.15.0",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "5.9.7",
        "@react-navigation/bottom-tabs": "^5.11.9",
        "@react-navigation/native": "^5.9.4",
        "@react-navigation/stack": "^5.14.4",
        "@types/react-native-htmlview": "^0.12.2",
        "@types/react-native-indicators": "^0.16.0",
        "axios": "^0.21.1",
        "expo": "^42.0.1",
        "expo-app-loading": "^1.0.1",
        "expo-constants": "^11.0.1",
        "expo-font": "^9.2.1",
        "expo-haptics": "~8.4.0",
        "expo-linear-gradient": "~8.4.0",
        "expo-linking": "^2.3.1",
        "expo-secure-store": "~9.3.0",
        "expo-status-bar": "~1.0.3",
        "expo-updates": "~0.4.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
        "react-native-elements": "^3.4.2",
        "react-native-gesture-handler": "~1.10.2",
        "react-native-reanimated": "~2.2.0",
        "react-native-safe-area-context": "3.2.0",
        "react-native-screens": "~3.4.0",
        "react-native-svg": "12.1.0",
        "react-redux": "^7.2.4",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "redux": "^4.0.5",
        "swr": "^0.5.5",
        "typesafe-actions": "^5.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --bail"
        }
    },
    "prettier": {
        "singleQuote": true,
        "printWidth": 140,
        "tabWidth": 4,
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "trailingComma": "es5"
    },
    "devDependencies": {
        "@babel/core": "~7.9.0",
        "@types/react": "~16.9.35",
        "@types/react-dom": "~16.9.8",
        "@types/react-native": "~0.63.2",
        "edit-json-file": "^1.6.0",
        "husky": "^5.0.9",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "typescript": "~4.0.0"
    },
    "private": true
}
